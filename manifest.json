{
  "name": "Trello Markdown Importer",
  "description": "Importa cartões e checklists de um arquivo Markdown para um quadro Trello.",
  "author": "Connect Digital Hub",
  "capabilities": {
    "board-buttons": [
      {
        "text": "Importar .md",
        "callback": "importMarkdown"
      }
    ],
    "card-buttons": [
      {
        "text": "Atualizar Checklist",
        "callback": "updateChecklist"
      }
    ],
    "card-badges": [],
    "card-back-section": {
      "title": "Importação .md",
      "icon": "https://3KLuke21.github.io/Md-importer-Trello/icon.png",
      "content": {
        "type": "iframe",
        "url": "https://3KLuke21.github.io/Md-importer-Trello/section.html",
        "height": 250
      }
    },
    "on-enable": {
      "url": "https://3KLuke21.github.io/Md-importer-Trello/on-enable.html"
    },
    "on-disable": {
      "url": "https://3KLuke21.github.io/Md-importer-Trello/on-disable.html"
    }
  },
  "connectors": {
    "iframe": {
      "url": "https://3KLuke21.github.io/Md-importer-Trello/index.html",
      "height": 500
    }
  },
  "version": "1.0"
}
